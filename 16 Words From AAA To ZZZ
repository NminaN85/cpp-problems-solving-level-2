
#include <iostream>
#include <cmath>
using namespace std;



void PrintWordsFromAAAToZZZ() {
	
		string Word = "";

	cout << "\n";

	for (int i = 65; i <= 90; i++) {
		for (int j = 65 ; j <= 90 ; j++) {
			for (int x = 65; x <= 90; x++) {


				Word = Word + (char)i;
				Word = Word + (char)j;
				Word = Word + (char)x;


				cout << Word << endl;
				Word = "";
			}
			
		}
	}

}



int main()
{
	PrintWordsFromAAAToZZZ();
			
		

	return 0;
}

