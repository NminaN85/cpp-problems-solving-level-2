
#include <iostream>
using namespace std;

void TableHeader() {
    cout << "\t\t\tMultiplication table From 1 to 10 " << endl;
    for (int i = 1; i <= 10; i++) {
        cout << "\t" << i;
    }
    cout << endl << "-----------------------------------------------------------------------------------" << endl;
}
void PrintMultiplicationTable() {
    TableHeader();

    for (int i = 1; i <= 10; i++) {
        cout << endl << i << "\t|";
        for (int j = 1; j <= 10; j++) {
            cout << i * j << "\t";
        }
    }
}

int main()
{
    PrintMultiplicationTable();
}

